    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.5.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.27",
    "@sonolus/core": "~7.12.0",
    "commander": "^13.1.0",
    "fs-extra": "^11.3.0"
  }
